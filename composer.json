{
    "name": "katenkka/enthuziast",
    "description": "A tool for (fan)listing collective owners to easily
maintain their listing collectives and listings under that collective",
    "license": "GPL-3.0-or-later",
    "type": "project",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "enthuziast/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
